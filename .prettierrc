{
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "jsxSingleQuote": false,
    "bracketSameLine": true,
    "printWidth": 150,
    "endOfLine": "auto",
    "importOrder": [
        "^[^/.]+$",
        "^@.+$",
        "^\\.\\./(.*)/[A-Z]((?!\\.)(?!types.).)+$",
        "^\\.\\./(.*)/((?!\\.)(?!types.).)+$",
        "^\\.$",
        "^\\./(.*)/[A-Z]((?!\\.)(?!types.).)+$",
        "^\\./(.*)/((?!\\.)(?!types.).)+$",
        "/types$",
        "\\.(css|scss)$",
        "\\.(svg|png|jpg|jpeg|gif)$",
        ".*"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
}
